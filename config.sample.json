{
	"sources": {
		"router": {
			"type": "snmp",
			"version": "v2c",
			"community": "public",
			"target": "10.0.0.1",
			"port": 161,
			"interfaceName": "eth0",
			"ipType": "A"
		},
		"routerv6": {
			"type": "snmp",
			"version": "v2c",
			"community": "public",
			"target": "10.0.0.1",
			"port": 161,
			"interfaceName": "eth1",
			"ipType": "AAAA"
		},
		"server": {
			"type": "dns",
			"dnsServer": "10.0.0.1",
			"domain": "server.internal",
			"ipType": "AAAA"
		},
		"static": {
			"type": "static",
			"ips": [
				"192.168.0.1"
			]
		},
		"webservicev4": {
			"type": "web",
			"url": "https://v4.ident.me/"
		},
		"webservicev6": {
			"type": "web",
			"url": "https://api64.ipify.org/"
		}
	},
	"transforms": {
		"trackip": {
			"type": "trackipv6",
			"trackSource": "routerv6",
			"trackIpSuffix": "::a6a2:0607:a36b:1929",
			"trackPrefixLength": 64,
			"ipSource": "server"
		}
	},
	"sinks": {
		"cloudflare": {
			"type": "cloudflare",
			"apiToken": "TOKEN",
			"zones": [
				{
					"name": "example.com",
					"records": [
						{
							"domain": "server",
							"sources": {
								"A": "router",
								"AAAA": "server"
							},
							"ttl": 60,
							"extra": {
								"proxied": false
							}
						}
					]
				}
			]
		},
		"technitium": {
			"type": "rfc2136",
			"host": "10.0.0.1",
			"keyName": "tsig-key",
			"keySecret": "TSIG-SECRET",
			"zones": [
				{
					"name": "example.com",
					"records": [
						{
							"domain": "server",
							"sources": {
								"A": "static",
								"AAAA": "trackip"
							},
							"ttl": 60
						}
					]
				}
			]
		}
	}
}
